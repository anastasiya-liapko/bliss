{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
