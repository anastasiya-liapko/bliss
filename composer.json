{
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.1.0",
    "ext-PDO": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-zip": "*",
    "ext-fileinfo": "*",
    "guzzlehttp/guzzle": "~6.0",
    "meng-tian/async-soap-guzzle": "*",
    "monolog/monolog": "*",
    "phpoffice/phpword": "^0.16.0",
    "rakit/validation": "*",
    "roave/security-advisories": "dev-master",
    "symfony/http-foundation": "^4.3",
    "twig/twig": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "Core/",
      "App\\": "App/",
      "Tests\\": "tests/"
    },
    "files": [
      "db.cfg.php"
    ]
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
